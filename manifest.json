{
	"manifest_version": 2,
	"name": "Torrent2Transmission",
	"description": "Adds a torrent from magnet link to Transmission daemon",
	"version": "0.1",
	"minimum_chrome_version": "40",
	"permissions": ["contextMenus","storage", "http://*/", "file://*/"],
	"background": {
		"persistent": false,
		"scripts": ["jquery-2.1.4.min.js","script.js"]
	},
	"icons": { 
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png" 
	},
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	}
}